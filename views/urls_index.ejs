<!DOCTYPE html>
<html lang="en">
<head>
  <%- include partials/_header %>
  <title>TinyApp</title>
</head>
<body>
    <ul><% for (url in urls){ %>
      <li>
        <%= url %> : <%= urls[url]%>
        <form method="POST" action="/urls/<%= url%>/delete">
           <a href="/urls/<%= url %>">Edit</a>
          <button> Delete </button>
        </form>
      </li>
    <% } %>
  </ul>
</body>
</html>