<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <%- include partials/_header %>
  <body>
    <ul><% for (url in urls) { %>
      <li>
        <%= url %> : <%= urls[url].longURL%>
        <form method="POST" action="/urls/<%= url%>/delete">
          <a href="/urls/<%= url %>">Edit</a>
          <button> Delete </button>
        </form>
        <% } %>
      </li>
    </ul>
    <% if(!!user) { %>
      <a href="/urls/new">Shorten Your URL!</a>
    <% } %>
  </body>
</html>